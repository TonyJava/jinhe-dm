<?xml version="1.0" encoding="UTF-8"?>
<Response>
	<PropertyInfo>
		<xform>
			<declare>
				<column name="id" caption="ID号" mode="string" editable="false"/>
				<column name="name" caption="名称" mode="string"/>
				<column name="value" caption="值" mode="string"/>
			</declare>
			<layout>
				<TR>
					<TD width="50"><label binding="id"/></TD>
					<TD><input binding="id" type="text"/></TD>
				</TR>
				<TR>
					<TD width="50"><label binding="name"/></TD>
					<TD><input binding="name" type="text"/></TD>
				</TR>
				<TR>
					<TD width="50" valign="top"><label binding="value"/></TD>
					<TD><input binding="value" type="text"/></TD>
				</TR>
			</layout>
			<data>
                <row></row>
			</data>
		</xform>
	</PropertyInfo>
</Response>