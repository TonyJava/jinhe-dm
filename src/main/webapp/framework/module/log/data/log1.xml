                </TR>
                <TR>
                    <TD width="50">
                        <label binding="target"/>
                    </TD>
                    <TD>
                        <input binding="target" type="text"/>
                    </TD>
                </TR>
                <TR>
                    <TD width="50">
                        <label binding="type"/>
                    </TD>
                    <TD>
                        <input binding="type" type="text"/>
                    </TD>
                </TR>
                <TR>
                    <TD width="50">
                        <label binding="date"/>
                    </TD>
                    <TD>
                        <input binding="date" type="text"/>
                    </TD>
                </TR>
				<TR>
                    <TD width="50">
                        <label binding="content"/>
                    </TD>
                    <TD>
                        <input binding="content" type="textarea"/>
                    </TD>
                </TR>
            </layout>
            <data>
