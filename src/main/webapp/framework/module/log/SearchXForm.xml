<?xml version="1.0" encoding="UTF-8"?>
<Response>
   <SearchLog>
    <xform>
			<declare>
				<column name="operatorName" caption="操作人" mode="string" />
				<column name="operatorIP" caption="操作人IP" mode="string" />
                <column name="operationCode" caption="操作类型" mode="string" />
                <column name="operateTable" caption="操作的表" mode="string" />
				<column name="startTime" caption="起始时间" mode="string" pattern="yyyy/MM/dd"/>
				<column name="endTime" caption="截止时间"   mode="string" pattern="yyyy/MM/dd"/>
			</declare>
			<layout>
				<TR>
					<TD width="50"><label binding="operatorName"/></TD>
					<TD><input binding="operatorName" type="text"/></TD>
				</TR>
				<TR>
					<TD width="50"><label binding="operatorIP"/></TD>
					<TD><input binding="operatorIP" type="text"/></TD>
				</TR>
				<TR>
					<TD width="50"><label binding="operationCode"/></TD>
					<TD><input binding="operationCode" type="text"/></TD>
				</TR>
				<TR>
					<TD width="50"><label binding="operateTable"/></TD>
					<TD><input binding="operateTable" type="text"/></TD>
				</TR>
				<TR>
					<TD width="50"><label binding="startTime"/></TD>
					<TD><input binding="startTime" type="text"/></TD>
				</TR>
				<TR>
					<TD width="50"><label binding="endTime"/></TD>
					<TD><input binding="endTime" type="text"/></TD>
				</TR>
			</layout>
			<data>
                <row/>
			</data>
		</xform>
    </SearchLog>
</Response>